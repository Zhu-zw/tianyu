.font1{
  font-size: 1.5rem;
  margin-left: 15rpx;
  margin-bottom: 15rpx;
}
#home-top1{
  width: 100%;
  height: 300rpx;
}
#home-top1 image{
  width: 100%;
  height: 100%;
}
#home-top2{
  width: 100%;
  height: 110rpx;
  border-bottom: #c1bfbf solid 2rpx; 
}
.location{
  width: 72%;
  height: 110rpx;
  float: left;
}
.loc-font1{
  font-size: 30rpx;
  float: left;
  width: 80%;
  margin-top:15rpx;
  margin-left: 10rpx;
}
.member{
  width: 28%;
  height: 110rpx;
  background-color: #007cc2;
/* 		background-image: url(../../static/images/member.jpg) ;
  background-size: 75rpx 75rpx;
  background-repeat: no-repeat; */
  float: left;
}
.member image{
  width: 100%;
  height: 100%;
  display: block;
}
.member p {
  font-size: 10rpx;
  padding-top: 0;
  text-align: center;
  color: white;
}
.search{
  width: 100%;
  height: 150rpx;
}
.search input{
  margin-top: 25rpx;
  margin-left: 50rpx;
  width: 500rpx;
  height: 80rpx;
  float: left;
  background-color: #f8f6f6;
  border-radius: 20rpx;
  vertical-align:middle;
  padding-left: 20rpx;
}
.search-btn{
  width: 100rpx;
  float: left;
  margin-top: 4%;
  margin-left: 5%;
}
.commodity{
  width: 100%;
  height: 800rpx;
  display: flex;
  background-color: #0077AA;
}
.left{
  height: 100%;
  width: 20%;
  background-color: #2C405A;
}
.right{
  height: 100%;
  width: 80%;
  background-color:#9A6E3A;
}
.add{
  width: 40rpx;
  height: 40rpx;
  float: right;
  margin-top: 5%;
}
.add image{
  width: 100%;
  height: 100%;
}

.place-order{
  width: 100%;
  height: 300rpx;
  background-color: #f6f6f6;
}
.order{
  width: 100%;
  height: 100rpx;
  background-color: white;
}
.shopping-img{
  width: 100rpx;
  height: 100rpx;
  float: left;
}
.shopping-img image{
  width: 80%;
  height: 80%;
  margin-top: 10rpx;
  margin-left: 20rpx;
}
.shopping-price{
  color: #e13035;
  width: 40%;
  float: left;
  font-size: 1.2rem;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: 40rpx;
}
.choice-y{
  width: 30%;
  height: 60rpx;
  background-color: #e13035;
  float: right;
  text-align: center;
  line-height: 60rpx;
  border-radius: 30rpx;
  color: white;
  margin-top: 20rpx;
  font-size: 0.8rem;
  margin-right: 40rpx;
}

.box {
  display: flex;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.left {
  width: 22vw;
}

.left-item {
  padding: 30rpx 20rpx;
  background-color: #eee;
}

.left-active {
  background-color: #fff;
  border-left: 6rpx solid red;
}

.right {
  width: 75vw;
  background: #fff;
  padding: 0 20rpx;
}


.flex-left{
  width: 85%;
  height: 220rpx;
  float: left;
}
.flex-right{
  width: 40rpx;
  height: 40rpx;
  margin-top:150rpx;
  float: right;
  margin-right: 5%;
}
.flex-image1{
  width:37%;
  height:80%;
  float: left;
}
.flex-right image{
  width: 100%;
  height: 100%;
}
.font2{
  float: left;
  width: 50%;
  font-size: 0.5rem;
  margin-left: 5%;
}
.font3{
  float: left;
