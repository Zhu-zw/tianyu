.top{
  width: 100%;
  height: 220rpx;
  background-color: white;
}
.portrait{
  float: left;
  width: 200rpx;
  height: 200rpx;
}
.portrait image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin-top: 20rpx;
  margin-left: 20rpx;
}
.username{
  width: 300rpx;
  height: 200rpx;
  float: left;
}
.username-p1{
  width: 200rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-top: 50rpx;
}
.username-p2{
  background-color: #f6c1bd;
  width: 150rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-top: 20rpx;
  font-size: 30rpx;
  text-align: center;
  border-radius: 20rpx;
  color: white;
}
.shopping-cart{
  width: 200rpx;
  height: 100rpx;
  float: right;
  margin-top: 80rpx;
  margin-right: 40rpx;
}
.shopping-cart view{
  text-align: center;
  border: #adacac solid 4rpx;
  border-radius: 10px;
  color: #686868;
  
}
.order{
  width: 100%;
